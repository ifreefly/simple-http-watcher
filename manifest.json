{
    "manifest_version": 2,

    "name": "simple-http-watcher",
    "description": "Like httpfox,but it's for chrome",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}